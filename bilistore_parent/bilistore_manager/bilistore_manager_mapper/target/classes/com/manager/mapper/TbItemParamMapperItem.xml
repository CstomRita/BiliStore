<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC
        "-//mybatis.org//DTDMapper3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.manager.mapper.TbItemParamItemMapper">
    <insert id="insertTbItemParamItem" useGeneratedKeys="true" keyProperty="id">
        insert into tb_item_param_item
        (
        item_id,param_data,created,updated
        )
        values (
        #{itemId},#{paramData},#{created},#{updated}
        )
    </insert>
</mapper>